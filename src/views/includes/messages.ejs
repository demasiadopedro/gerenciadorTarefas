<% if(errors.length > 0){%>
<div class="row">
    <div class="col">
        <div class="alert alert-danger">
            <%errors.forEach(erro=>{%>
                <%= erro %>
            <%})%>
        </div>
    </div>
</div>  
<%}%>

<% if(success.length > 0){%>
<div class="row">
    <div class="col my-3">
        <div class="alert alert-success">
            <%success.forEach(certo=>{%>
                <%= certo %>
            <%})%>
        </div>
    </div>
</div>  
<%}%>
